require Inheritance;

operator entry() {
  SimpleParent p(5);
  report('p.getA() = '+p.getA());
  report('p.getSum() = '+p.getSum());

  SimpleChild c(1, 3);
  report('c.getA() = '+c.getA());
  report('c.getB() = '+c.getB());
  report('c.getSum() = '+c.getSum());

  NoMemberParent np(8);
  NoMemberChild nc(9);
  if (nc.dataSize() != np.dataSize() )
    throw ("Data size mismatch!");
  report('nc.dataSize() = '+nc.dataSize());
  report('nc.getA() = '+nc.getAPlusFive());

  NotAbstract na();
  na.doNothing();
  AbstractParent ap(na);
  ap.doNothing();
  AbstractChild ac(na);
  ac.doNothing();
  AbstractChild ac2 = na.getChild();
  AbstractParent ap2 = na.getParent();
  TakesAbstract(na);
  TakesAbstract(ac);
  TakesAbstract(ap);
}
