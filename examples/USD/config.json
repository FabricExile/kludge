{
  "extname": "USD",
  "infiles": [
    "custom.h",
    "${USD_INSTALL_ROOT}/include/pxr/base/tf/declarePtrs.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/common.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/sdf/path.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/sdf/layer.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/timeCode.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/attribute.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/relationship.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/property.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/prim.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usd/stage.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usdGeom/mesh.h",
    "${USD_INSTALL_ROOT}/include/pxr/usd/usdGeom/xform.h"
    ],
  "cpppath": [
    "${USD_INSTALL_ROOT}/include",
    "${USD_INSTALL_ROOT}/include/gcc4-fixes",
    "/usr/include/python2.7",
    "/usr/include/OpenEXR",
    "/opt/usd-deps/root/usr/include",
    "/build/fabric-llvm/build/lib/clang/3.9.0/include"
    ],
  "libpath": [
    "${USD_INSTALL_ROOT}/lib",
    "/opt/usd-deps/root/usr/lib"
    ],
  "clang_opts": [
    "-x", "c++",
    "-std=c++11",
    "-m64",
    "-DBUILD_OPTLEVEL_DEV",
    "-DBUILD_COMPONENT_SRC_PREFIX=\"pxr\"",
    "-DNPY_NO_DEPRECATED_API=NPY_1_7_API_VERSION",
    "-DBOOST_PYTHON_NO_PY_SIGNATURES",
    "-DLINUX",
    "-DFEDORA",
    "-DGPLUSPLUS",
    "-DOS_LINUX",
    "-DMFB_EXTERNAL_DIST",
    "-DPOSIX_THREADS",
    "-D_ISOC99_SOURCE",
    "-D_XOPEN_SOURCE=600",
    "-D_GNU_SOURCE",
    "-DSYS_V",
    "-D_REENTRANT",
    "-D__STDC_LIMIT_MACROS",
    "-D__null=0"
    ],
  "libs": [
    "usd",
    "usdGeom",
    "sdf",
    "tf"
    ],
  "ignore_methods": [
    "find",
    "begin",
    "end"
    ],
  "ignore_types": [
    "_Rep",
    "_TypeInfo",
    "PcpNodeRef",
    "SdfTupleDimensions",
    // FIXME output issue with KL::FixedArray with these arrays
    "GfMatrix3d",
    "GfMatrix4d"
    ],
  "force_op_arrow": [
    "TfWeakPtr"
    ],
  "kl_files": [
    "Conversions.kl"
    ]
  }
