{% extends "decl.template.kl" %}
{% block body %}
struct {{ decl.self_type_name.kl.compound }} {
  private Data cpp_ptr;
};

  {% for member in decl.members %}
    {% if member.is_public %}
{{ member.codec.type.kl.compound }} {{ decl.self_type_name.kl.compound }}.GET_{{ member.codec.name.kl }}() = "{{ decl.self_type_name.kl.compound }}_GET_{{ member.codec.name.kl }}";
{{ decl.self_type_name.kl.compound }}.SET_{{ member.codec.name.kl }}!({{ member.codec.type.kl.base }} {{ member.codec.name.kl }}{{ member.codec.type.kl.suffix }}) = "{{ decl.self_type_name.kl.compound }}_SET_{{ member.codec.name.kl }}";

    {% endif %}
  {% endfor %}

  {% for method in decl.methods %}
    {% if method.result_codec %}
{{ method.result_codec.type.kl.compound }}
    {% endif %}
{{ decl.self_type_name.kl.compound }}.{{ method.name }}(
    {% for param in method.params %}
    {{ param.param_kl() }}{{ "," if not loop.last else "" }}
    {% endfor %}
    ) = "{{ decl.self_type_name.kl.compound }}_{{ method.name }}";

  {% endfor %}
{% endblock body %}
