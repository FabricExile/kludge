require StdString;

operator entry() {
  String mutableRef = "mutableRef";
  String mutablePtr = "mutablePtr";
  report("Before: mutableRef = " + mutableRef);
  report("Before: mutablePtr = " + mutablePtr);
  StdStringParams(
    "value",
    "constRef",
    "constPtr",
    mutableRef,
    mutablePtr
    );
  report("After: mutableRef = " + mutableRef);
  report("After: mutablePtr = " + mutablePtr);

  report("StdStringValueReturn() = " + StdStringValueReturn());
  report("StdStringConstRefReturn() = " + StdStringConstRefReturn());
  report("StdStringMutableRefReturn() = " + StdStringMutableRefReturn());
}
